<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<mapping>
		<class-a>org.backmeup.model.BackupJob</class-a>
		<class-b map-null="false">org.backmeup.model.dto.BackupJobDTO</class-b>
		
		<field>
  			<a>id</a>
  			<b>jobId</b>
		</field> 
		<field>
  			<a>jobName</a>
  			<b>jobTitle</b>
		</field> 
		<field>
  			<a>createTime</a>
  			<b>created</b>
		</field> 
		<field>
  			<a>lastUpdatedTime</a>
  			<b>modified</b>
		</field> 
		<field>
  			<a>status</a>
  			<b>jobStatus</b>
		</field> 
		<field>
  			<a>timeExpression</a>
  			<b>schedule</b>
		</field> 
		<field>
  			<a>nextExecutionTime</a>
  			<b>next</b>
		</field> 
	</mapping>
	
	<mapping>
		<class-a>org.backmeup.model.BackupJobExecution</class-a>
		<class-b map-null="false">org.backmeup.model.dto.BackupJobExecutionDTO</class-b>
		
		<field>
  			<a>backupJobId</a>
  			<b>jobId</b>
		</field>
		
		<field>
  			<a>createTime</a>
  			<b>created</b>
		</field>
		
		<field>
  			<a>lastUpdated</a>
  			<b>modified</b>
		</field>
		
		<field>
  			<a>startTime</a>
  			<b>start</b>
		</field>
		
		<field>
  			<a>endTime</a>
  			<b>end</b>
		</field>
		
		<field>
  			<a>sourceProfile</a>
  			<b>source</b>
		</field>
		
		<field>
  			<a>actionProfiles</a>
  			<b>actions</b>
		</field>
		
		<field>
  			<a>sinkProfile</a>
  			<b>sink</b>
		</field>
	</mapping>
</mappings> 